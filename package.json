{
  "name": "duncan",
  "type": "module",
  "version": "0.0.3",
  "description": "Great and Responsable TypeScript Chains",
  "keywords": [
    "duncan",
    "chain",
    "responsability",
    "sequential",
    "parallel",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "author": "Jérémie Pichon <contact@cadgerfeast.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/dts/index.d.ts"
    }
  },
  "unpkg": "dist/umd/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/dts/index.d.ts",
  "scripts": {
    "dev": "madoc dev",
    "build": "run-s build:*",
    "build:lib": "run-p build:lib:*",
    "build:lib:esm": "tsc -p tsconfig.esm.json",
    "build:lib:cjs": "tsc -p tsconfig.cjs.json",
    "build:lib:umd": "rollup -c rollup.config.js",
    "build:lib:dts": "tsc -p tsconfig.dts.json",
    "build:docs": "madoc build",
    "test": "jest",
    "lint": "eslint ./src ./test"
  },
  "devDependencies": {
    "@madoc/core": "^0.1.8",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.9.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
